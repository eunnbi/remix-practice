{
  "name": "remix-jokes",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "remix vite:dev",
    "build": "remix vite:build",
    "start": "remix-serve ./build/server/index.js",
    "format": "biome format --write ./app ./prisma",
    "lint": "eslint --ignore-pattern .gitignore --cache --cache-location ./node_modules/.cache/eslint ./app",
    "lint:fix": "eslint --fix ./app",
    "typecheck": "tsc",
    "prisma:migrate": "pnpm prisma migrate dev"
  },
  "dependencies": {
    "@prisma/client": "^6.2.0",
    "@remix-run/node": "^2.15.2",
    "@remix-run/react": "^2.15.2",
    "@remix-run/serve": "^2.15.2",
    "bcryptjs": "^2.4.3",
    "@remix-practice/design-system": "workspace:*",
    "isbot": "^5.1.20",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tiny-invariant": "^1.3.3",
    "valibot": "1.0.0-beta.11"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.15.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.3",
    "@types/react-dom": "^19.0.2",
    "@vanilla-extract/css": "^1.17.0",
    "@vanilla-extract/vite-plugin": "^4.0.19",
    "eslint": "^9.17.0",
    "@remix-practice/eslint-config": "workspace:*",
    "prisma": "^6.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vite": "^6.0.7",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
